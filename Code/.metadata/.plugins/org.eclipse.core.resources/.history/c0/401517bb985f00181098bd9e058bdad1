
public class Main {

	public static void main(String[] args) {
		int[] arr = {0,1,0,2,1,0,1,3,2,1,2,1};
		getVolume(arr);
	}
	
	static int getVolume(int[] alturas){
		int[] maximoPelaEsquerda = new int[alturas.length];
		int[] maximoPelaDireita = new int[alturas.length];
		int[] maximoJunto = new int[alturas.length];
		
		int oMaisAltoAteAgora = 0;
		for(int i = 0; i < alturas.length; i++){
			if(alturas[i] > oMaisAltoAteAgora)
				oMaisAltoAteAgora = alturas[i];
		}
		return 0;
	}
}
