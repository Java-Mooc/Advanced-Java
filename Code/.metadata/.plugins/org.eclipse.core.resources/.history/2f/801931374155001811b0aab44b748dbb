public class Palindrome {
    public static boolean isPalindrome(String word) {
        String word2 = "";
        for(int i = 0; i < word.lenght(); i++)
            word2 = word[i] + word2;
        return word
    }
    
    public static void main(String[] args) {
        System.out.println(Palindrome.isPalindrome("Deleveled"));
    }
}