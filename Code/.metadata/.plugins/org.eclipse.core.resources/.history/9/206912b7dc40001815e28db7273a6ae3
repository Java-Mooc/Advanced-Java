package ppt1;

import java.net.URL;

public class Main {

	public static void main(String[] args) {
		try {
			//A tentar conectarmos com o url
			URL url = new URL("https://www.google.com");
			
			System.out.println(url.getContent());
		} catch (Exception e){
			System.err.println("Ocorreu uma excepção!\n" + e.getMessage());
		} 

	}

}
